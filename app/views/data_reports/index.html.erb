<% if flash[:error] %><p class="flash_error"><%=flash[:error]%></p><% end %>

<% form_tag :action => :volunteer_report do -%>
Please Select a range for the Volunteer Report 
<br />
From year: <%= select_year Time.now.year, {:start_year => Time.now.year-10, :end_year => Time.now.year}, :name => 'v_start' %>&#x00A;
To year: <%= select_year Time.now.year, {:start_year => Time.now.year-10, :end_year => Time.now.year}, :name => 'v_end' %>&#x00A;
<%= submit_tag "Submit" %> 
<% end -%>

<%= link_to 'Volunteer Data', :action => :volunteer_report %> |
<%= link_to 'Projects Data', :action => :projects_report %> |
<%= link_to 'Back', :controller => :s_accounts, :action => :my_account %>